//primeiro teste desenho livre, animaçãolouca... 
//vamos ver no que vai dar

void setup() 
{
  
size(800,800);
background(50);
noStroke();
frameRate(25);

}

void draw() 
{
background(100,92);
rectMode(CORNERS);
fill(200,0,100);
rect(30,30,random(600),600,8);
fill(155,200);
ellipse(250,400,400,400);
fill(0,50);
ellipse(random(width),random(height),120,180);
ellipse(random(width),random(height),random(250),random(250));
fill(80,200,120,80);
ellipse(random(width),45,70,70);


}
